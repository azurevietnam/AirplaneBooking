<link rel="stylesheet" href="example_files/subModal.css" type="text/css"><script language="JavaScript" type="text/javascript">
function SubmitForm() {
   if (CSValidation()) {
        if (typeof(goLoading) != 'undefined') {
            goLoading();
        }
       pop('?lang=vi');
       setTimeout("document.forms['TravelOptions'].submit()", 100);
   }
}
function ResetForm() {
    if (typeof(goLoading) != 'undefined') {
        goLoading();
    }
   pop('?lang=vi');
   setTimeout("document.forms['TravelOptions'].submit()", 100);
}
</script>
